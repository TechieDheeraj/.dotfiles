{
    "workbench.colorTheme": "Default Dark+",
    "terminal.integrated.fontSize": 14,
    "editor.fontLigatures": false,
    "editor.minimap.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "editor.fontSize": 14,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.enablePreview": false,
    "workbench.iconTheme": "Monokai Pro Icons",
    "window.zoomLevel": 1,
    "yaml.schemas": {
        "file:///Users/dheeraj/.vscode/extensions/docsmsft.docs-yaml-1.0.1/dist/toc.schema.json": "/toc\\.yml/i"
    },
    "terminal.external.osxExec": "iTerm.app",
    "terminal.explorerKind": "external",
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "args": [
                "-l"
            ],
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh",
            "args": [
                "-l"
            ]
        },
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "workbench.colorCustomizations": {},
    "playwright.reuseBrowser": true,
    "playwright.showTrace": false,
    "playwright.env": {},
    "git.openRepositoryInParentFolders": "never",
    "gitlens.graph.dateFormat": "",
    "[python]": {
        "editor.formatOnType": true
    },
    "python.terminal.activateEnvironment": true,
    "python.venvPath": "/Users/r00tdk/Documents/code/venv",
    "python.defaultInterpreterPath": "/Users/dheeraj/Documents/code/Jenkins-Configuration-as-code/DevOps/environment-managment/new_cluster_generation/venv/bin/python",
    "jupyter.notebookFileRoot": "${workspaceRoot}",
    "cmake.showOptionsMovedNotification": false,
    "security.workspace.trust.untrustedFiles": "open",
    "debug.allowBreakpointsEverywhere": true,
    "debug.showBreakpointsInOverviewRuler": true,
    "lldb.launch.breakpointMode": "path",

    // Golang Settings
    "go.useLanguageServer": true,
    "[go]": {
        "editor.formatOnSave": true
    },
    "gopls": {
        "usePlaceholders": true,
        "staticcheck": false,
        "deepCompletion": true,
        "completeUnimported": true
    },
    "go.toolsManagement.autoUpdate": true,
    "go.toolsEnvVars": {
        "GO111MODULE": "on"
    },
}
